{
  "name": "rnet-proxy",
  "version": "1.1.1",
  "description": "Proxy between smart devices and RNet enabled amps",
  "main": "src/app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "pm2 start src/app.js --name rnet-pi",
    "stop": "pm2 stop rnet-pi",
    "restart": "pm2 restart rnet-pi",
    "dev": "node src/app.js",
    "docker:build": "docker build -t rnet-pi .",
    "docker:run": "docker run -p 3000:3000 --device=/dev/ttyUSB0:/dev/ttyUSB0 rnet-pi"
  },
  "author": "zachcheatham",
  "license": "ISC",
  "dependencies": {
    "amator": "^1.1.0",
    "bonjour-service": "^1.3.0",
    "castv2-client": "^1.2.0",
    "console-stamp": "^3.1.2",
    "express": "^5.0.1",
    "pm2": "^6.0.13",
    "serialport": "^11.0.1",
    "smart-buffer": "^4.2.0",
    "websocket": "^1.0.35"
  }
}
